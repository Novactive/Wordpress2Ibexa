{
    "name": "almaviacx/wordpress-ibexa",
    "description": "Convert WordPress Posts to ibexa Contents",
    "keywords": [
        "ezcommerce",
        "ibexa"
    ],
    "authors": [
        {
            "name": "Almaviacx",
            "email": "dir.tech@novactive.com"
        },
        {
            "name": "Ousmane KANTE",
            "email": "ousmane.kante@almaviacx.com"
        }
    ],
    "type": "ibexa-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-zip": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "squizlabs/php_codesniffer": "^3.6",
        "ibexa/oss-skeleton": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Almaviacx\\Bundle\\Ibexa\\WordPress\\": "bundle/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "ibexa/post-install": true
        }
    }
}
